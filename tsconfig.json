{
  "compilerOptions": {
    "noEmit": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "inlineSourceMap": true,
    "inlineSources": true,
    "module": "ESNext",
    "target": "ESNext",
    "allowJs": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "isolatedModules": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "lib": [
      "DOM",
      "ES5",
      "ES6",
      "ES7",
      "ESNext"
    ],
    "types": [
      "obsidian-typings"
    ]
  },
  "include": [
    "**/*.ts"
  ]
}